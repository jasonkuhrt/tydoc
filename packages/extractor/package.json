{
  "name": "@tydoc/extractor",
  "description": "TyDoc API for getting extracted documentation data (EDD)",
  "version": "0.2.1",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "types.d.ts",
    "types.js"
  ],
  "scripts": {
    "build:docs:toc": "doctoc README.md",
    "build:docs:api": "yarn g:ts-node scripts/build-api-docs",
    "build:docs": "yarn build:docs:api && yarn build:docs:toc",
    "build": "yarn g:tsc --build && echo 'done build'",
    "dev": "yarn g:tsc --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "jest",
    "test:debug": "DEBUG='*' DEBUG_COLORS=true jest --watch",
    "format": "yarn g:prettier --write .",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.12.21",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.165",
    "chaindown": "^0.1.0-next.1",
    "debug": "^4.3.1",
    "dedent": "^0.7.0",
    "doctoc": "^1.4.0",
    "fs-jetpack": "^4.1.0",
    "globby": "^11.0.1",
    "got": "^11.8.0",
    "json5": "^2.1.3",
    "kleur": "^4.1.3",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "ts-morph": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/decompress": "^4",
    "@types/dedent": "^0.7.0",
    "@types/jest": "26.0.15",
    "@types/json5": "^0.0.30",
    "@types/node": "14.14.10",
    "@types/prettier": "2.1.5",
    "decompress": "^4.2.1",
    "jest": "26.6.3",
    "jest-validate": "26.6.2",
    "jest-watch-suspend": "1.1.2",
    "jest-watch-toggle-config": "2.0.1",
    "jest-watch-typeahead": "0.6.1",
    "ts-jest": "26.4.4",
    "type-fest": "^0.20.2"
  },
  "prettier": "@prisma-labs/prettier-config"
}
