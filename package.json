{
  "name": "docs",
  "version": "0.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "doc": "dist/cli/main.js"
  },
  "files": [
    "dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build:docs": "node dist/cli/main module src",
    "build:oclif": "oclif-dev manifest",
    "build:ts": "tsc",
    "build": "yarn -s build:oclif && yarn -s build:ts",
    "dev": "tsc -w",
    "clean": "rm -rf dist",
    "dev:test": "jest --watch",
    "test": "jest",
    "test:dev": "yarn -s dev:test"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@types/node": "^13.1.4",
    "fs-jetpack": "^2.2.3",
    "globby": "^11.0.0",
    "ts-morph": "^6.0.2",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/jest": "^25.1.2",
    "@types/prettier": "^1.19.0",
    "jest": "^25.1.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^1.0.2",
    "jest-watch-typeahead": "^0.4.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "ts-jest",
    "watchPlugins": [
      "jest-watch-suspend",
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname",
      [
        "jest-watch-toggle-config",
        {
          "setting": "verbose"
        }
      ]
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/oclif.manifest.json"
    ],
    "setupFiles": [
      "./test/setup.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json",
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "oclif": {
    "commands": "./dist/cli/commands",
    "bin": "doc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
